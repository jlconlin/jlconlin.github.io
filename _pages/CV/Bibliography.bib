@inproceedings{No:2005Very--0,
	abstract = {Very-high-temperature gas-cooled reactors (VHTGRs) are gaining increasing attention due to their technical advantages over light water reactors in terms of intrinsic safety and proliferation-resistant fuel cycle as well as hydrogen production capability. The objective of this paper is to report on the progress of a US/Korea I-NERI research project and address some significant issues and challenges. New advanced computational methods for safety analysis codes for VHTGRs have been developed and numerical and experimental validation of these computer codes was performed\textbf{. }In particular, two well-respected light water reactor transient response codes, RELAP5/ATHENA and MELCOR, were improved in the modeling of molecular diffusion and chemical equilibrium with the aim to validate these codes against the VHTGR accident data, i.e., air ingress and others from the literature. This study consists of five areas: (1) development of computational fluid dynamic methods for VHTGR, (2) experimental study of reactor cavity cooling system, (3) air ingress experiment, (4) theoretical modification of system analysis codes for molecular diffusion (RELAP5/ATHENA) and modeling CO and CO2 equilibrium (MELCOR), (5) development of a state-of-the-art methodology for VHTGR neutronic analysis and calculation of accurate power distributions and decay heat deposition rates.},
	address = {Seoul, Korea},
	author = {Hee-Cheon No and Chang H. Oh and Jong H. Kim and Goon-Cherl Park and John C. Lee and William R. Martin and Hong Sik Lim and Eung Soo Kim and M. O. Kim and Jeremy L. Conlin and Wei Ji},
